//SYSP08SM JOB (40208),SCHIRADIN,MSGCLASS=R,MSGLEVEL=(1,1),
//             CLASS=A,PRTY=15,NOTIFY=&SYSUID,REGION=60M       **
//*
//*       BUILD SHOWMVS.MACLIB LIBRARY FOR SHOWMVS
//*
//IDCCAMS EXEC PGM=IDCAMS
//SYSPRINT  DD SYSOUT=*
//SYSIN  DD *
   DELETE 'SYSP08.SHOW724.MACLIB' NONVSAM
   SET MAXCC=0
//PVTMACS EXEC PGM=IEBUPDTE,PARM=NEW
//SYSUT2   DD DSN=SYSP08.SHOW724.MACLIB,
// DISP=(MOD,CATLG),
// DSORG=PO,RECFM=FB,LRECL=80,
// UNIT=SYSALLDA,
// SPACE=(TRK,(10,5,30))
//SYSPRINT DD SYSOUT=*
./ ADD NAME=$$README

