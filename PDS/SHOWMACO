./ ADD NAME=DIAG
         MACRO
&NAME    DIAG &RX,&RY,&C               Generate DIAGNOSE Instruction
&NAME    DC   X'83',AL1(&RX*16+&RY),S(&C)
         MEND
./ ADD NAME=DIAG204
         MACRO
         DIAG204
D204     DSECT                         Data from DIAGNOSE X'204'
D204HDR  DS   0XL16                    TABLE HEADER
D204LEN  DS    H,H                     LENGTH OF D204 TABLE
D204PCCT DS    H'8'                    NUMBER OF PHYSICAL CPU'S
D204OWNP DS    Y(D204LPAR-D204)        OFFSET TO OWN LPAR
D204STCK DS    XL8                     LAST DIAG204'S TOD CLOCK VALUE
D204LPAR DS   0XL16                    LPAR HEADER
D204LPNO DS    X                       LOGICAL PARTITION NUMBER
D204LPCP DS    X                       NUMBER OF CPUS FOR THIS LPAR
         DS    H,F                     RESERVED
D204LPNM DS    CL8                     LPAR NAME
D204LPAD DS    H,H,H                   CPU ADDRESS
D204LPRS DS    H'25'                   PROCESSOR RELATIVE SHARE
D204LPT1 DS    XL8                     LPAR TOTAL DISPATCH TIME
D204LPT2 DS    XL8                     LPAR EFFECTIVE DISPATCH TIME
D2047     DSECT                         Data from DIAGNOSE X'204'
D2047HDR  DS   0XL64                    TABLE HEADER
D2047LEN  DS    H,H                     LENGTH OF D204 TABLE
D2047PCCT DS    H'8'                    NUMBER OF PHYSICAL CPU'S
D2047OWNP DS    Y(D204LPAR-D204)        OFFSET TO OWN LPAR
          DS    X
D2047STCK DS    XL8                     LAST DIAG204'S TOD CLOCK VALUE
          DS    CL63                    ???
D2047LPAR DS   0XL96                    LPAR HEADER
D2047LPNO DS    X                       LOGICAL PARTITION NUMBER
D2047LPCP DS    X                       NUMBER OF CPUS FOR THIS LPAR
D2047LPEN DS    X                       Number of CPUs entries
          DS    X,F                     RESERVED
D2047LPNM DS    CL8                     LPAR NAME
D2047SYSID DS    CL8                    SYSID NAME
D2047SYS  DS    CL8                     SYSTEM NAME
          DS    CL64
D2047LPAD DS    H,H                     CPU ADDRESS Entry
D2047CPF1 DS    X                       Flag ??
D2047CPF2 DS    X                       Flag ??
D2047LPRS DS    H'25'                   PROCESSOR RELATIVE SHARE
D2047LPT1 DS    XL8                     LPAR TOTAL DISPATCH TIME
D2047LPT2 DS    XL8                     LPAR EFFECTIVE DISPATCH TIME
          DS    H
         MEND
