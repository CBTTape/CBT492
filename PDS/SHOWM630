//IBMUSERM JOB (ACCT#),SHOWM628,
// NOTIFY=&SYSUID,
// CLASS=A,MSGCLASS=X,COND=(0,NE)
//*
//*       BUILD SHOWMVS.MACLIB LIBRARY FOR SHOWMVS V628
//*
//CLEANUP EXEC PGM=CLEANUP                                        -DEL-
//IDCCAMS EXEC PGM=IDCAMS                                         -DEL-
//SYSPRINT  DD SYSOUT=*
//SYSIN  DD *
   DELETE 'IBMUSER.SHOW628.MACLIB' NONVSAM
   SET MAXCC=0
//PVTMACS EXEC PGM=IEBUPDTE,PARM=NEW
//SYSUT2   DD DSN=IBMUSER.SHOW628.MACLIB,
// DISP=(MOD,CATLG),
// DSORG=PO,RECFM=FB,LRECL=80,
// UNIT=SYSALLDA,
// SPACE=(TRK,(6,2,5))
//SYSPRINT DD SYSOUT=*
./ ADD NAME=$$README

