./ ADD NAME=IGDDCD
* %GOTO DCDF1;                      /*
                                       MACRO
                                       IGDDCD
                                       AGO   .SSXXASM
*                                                                    */
* %DCDF1: ;
*/********************************************************************/
*/*  MACRO NAME: IGDDCD                                              */
*/*                                                                  */
*/*  DESCRIPTION: DATA CLASS MAPPING                                 */
*/*                                                                  */
*/*  PERSON RESPONSIBLE: R. L. FERZIGER                              */
*/*                                                                  */
*/*  STATUS: DFP/XA 3.1.0                                            */
*/*                                                                  */
*/*  5665-XA3                                                        */
*/*  CONTAINS RESTRICTED MATERIALS OF IBM                            */
*/*  (C) COPYRIGHT IBM CORP. 1988                                    */
*/*  LICENSED MATERIALS - PROPERTY OF IBM                            */
*/*  REFER TO COPYRIGHT INSTRUCTIONS                                 */
*/*  FORM NUMBER G120-2083.                                          */
*/*                                                                  */
*/*  FUNCTION: MAPS DEFINITION OF DATA CLASSES.                      */
*/*                                                                  */
*/*  NOTES:                                                          */
*/*                                                                  */
*/*    DEPENDENCIES: NONE                                            */
*/*    RESTRICTIONS: BECAUSE GETMAINS AND FREEMAINS MAY BE DONE      */
*/*      FOR THE INFORMATION MAPPED BY THIS MACRO, THE MACRO MUST    */
*/*      BE FORCED TO A DOUBLEWORD BOUNDARY.                         */
*/*                                                                  */
*/*  MACRO:                                                          */
*/*                                                                  */
*/*    PROCESSOR: PLAS/ASSEMBLER                                     */
*/*    MACRO SIZE: 115                                               */
*/*    DISTRIBUTION LIBRARY: MACLIB                                  */
*/*    ATTRIBUTES: BILINGUAL                                         */
*/*                                                                  */
*/*      THIS MACRO PRODUCES A PLS STRUCTURE OR AN ASSEMBLER DSECT   */
*/*      WHICH DESCRIBES THE FORMAT OF DATA CLASSES.                 */
*/*                                                                  */
*/*  EXIT NORMAL:                                                    */
*/*                                                                  */
*/*    RETURN CODE: 0                                                */
*/*    NO ERRORS DETECTED                                            */
*/*                                                                  */
*/*  EXIT ERRORS:                                                    */
*/*                                                                  */
*/*      NO ERRORS WILL BE DETECTED BY THIS MACRO.                   */
*/*                                                                  */
*/*  EXTERNAL REFERENCES:                                            */
*/*                                                                  */
*/*    MACROS:                                                       */
*/*      NONE                                                        */
*/*                                                                  */
*/*  CHANGE ACTIVITY:                                                */
*/********************************************************************/
*@EJECT M;
*/********************************************************************/
*/*                                                                  */
*/*      SMS DATA CLASS CONSTRUCT DEFINITION                         */
*/*                                                                  */
*/********************************************************************/
* DECLARE
*   /*    DATA    CLASS DEFINITION                                   */
*   1 DCD BASED BDY(DWORD),         /* DATA CLASS MAPPING            */
*     2 DCDP,                       /* DATA CLASS PREFIX             */
*       3 DCDPID CHAR(8),           /* ID = 'IGDDCD'                 */
*       3 *       CHAR(2),          /* UNUSED                        */
*       3 DCDPVER FIXED(15),        /* VERSION OF MACRO              */
*       3 DCDPCNT FIXED(31),        /* NUMBER OF DC DEFS -- COUNT    */
*       3 DCDPTYP FIXED(15),        /* TYPE OF ITEMS IN LIST         */
*       3 *       CHAR(2),          /* RESERVED                      */
*       3 DCDPLEN FIXED(31),        /* LENGTH OF ONE DEF -- DCDEF    */
*     2 * (*) BDY(DWORD),           /* ARRAY OF DATA CLASS DEFS      */
*      3  DCDEF BDY(DWORD),
*       4 DCDNM,                    /* SPACE FOR NAME AND LENGTH     */
*         5 DCDNMLEN FIXED(15),     /* LENGTH OF NAME                */
*         5 DCDNAME CHAR(30),       /* NAME OF DATA CLASS            */
*       4 DCDUSER CHAR(8),          /* USERID OF LAST UPDATER        */
*       4 DCDDATE CHAR(10),         /* DATE OF LAST UPDATE           */
*       4 *       CHAR(6),          /* RESERVED                      */
*       4 DCDTIME CHAR(8),          /* TIME OF LAST UPDATE           */
*       4 DCDDESC CHAR(120),        /* DESCRIPTION                   */
*       4 DCDSUBDF,                 /* SUB DEFINITION                */
*         5 DCDDS CHAR(4),          /* DATA SET ATTRIBUTES           */
*           6 DCDRCORG FIXED(8),    /* DATA SET RECORG -- SEE        */
*                                   /* CONSTANTS BELOW               */
*           6 DCDRECFM FIXED(8),    /* DATA SET RECFM -- SEE         */
*                                   /* CONSTANTS BELOW               */
*           6 *,
*             7 DCDBLK   BIT(1),    /* 1 = BLOCKED, 0 = UNBLKED/NULL */
*             7 DCDSTSP  BIT(1),    /* 1 = STANDARD OR SPANNED,ELSE 0*/
*             7 *        BIT(6),    /* RESERVED                      */
*           6 DCDCNTL  FIXED(8),    /* CARRIAGE CONTROL -- SEE       */
*                                   /* CONSTANTS BELOW               */
*         5 DCDSPEC CHAR(3),        /* SPECIFIED FIELDS FLAGS:       */
*                                   /* 1 = SPECIFIED, 0 = NOT SPEC'D */
*           6 DCDFRORG BIT(1),      /* RECORG SPECIFIED FLAG         */
*           6 DCDFLREC BIT(1),      /* LRECL SPECIFIED FLAG          */
*           6 DCDFRFM BIT(1),       /* RECFM SPECIFIED FLAG          */
*           6 DCDFKLEN BIT(1),      /* KEYLEN SPECIFIED FLAG         */
*           6 DCDFKOFF BIT(1),      /* KEYOFF SPECIFIED FLAG         */
*           6 DCDFEXP BIT(1),       /* EXPIRATION ATTRIB SPEC'D FLAG */
*           6 DCDFRET BIT(1),       /* RETENTION  ATTRIB SPEC'D FLAG */
*           6 DCDFPSP BIT(1),       /* PRIMARY SPACE SPECIFIED FLAG  */
*           6 DCDFSSP BIT(1),       /* SECONDARY SPACE SPEC'D FLAG   */
*           6 DCDFDIR BIT(1),       /* DIRECTORY BLOCKS SPEC'D FLAG  */
*           6 DCDFAUN BIT(1),       /* ALLOCATION UNIT SPEC'D FLAG   */
*           6 DCDFAVR BIT(1),       /* AVGREC SPECIFIED FLAG         */
*           6 DCDFVOL BIT(1),       /* VOLUME CNT SPECIFIED FLAG     */
*           6 DCDFCIS BIT(1),       /* CIZIZE SPECIFIED FLAG         */
*           6 DCDFCIF BIT(1),       /* FREE CI % SPECIFIED FLAG      */
*           6 DCDFCAF BIT(1),       /* FREE CA % SPECIFIED FLAG      */
*           6 DCDFXREG BIT(1),      /* SHAREOPT XREGION SPEC'D FLAG  */
*           6 DCDFXSYS BIT(1),      /* SHAREOPT XSYSTEM SPEC'D FLAG  */
*           6 DCDFIMBD BIT(1),      /* VSAM IMBED SPECIFIED FLAG     */
*           6 DCDFRPLC BIT(1),      /* VSAM REPLICATE SPECIFIED FLAG */
*           6 *        BIT(4),      /* RESERVED                      */
*         5 * CHAR(1),              /* RESERVED                      */
*         5 DCDKEY,                 /* VSAM KEY INFORMATION          */
*           6 DCDKOFF FIXED(15),    /* VSAM KEY OFFSET               */
*           6 DCDKLEN FIXED(8),     /* VSAM KEY LENGTH               */
*           6 *     FIXED(8),       /* RESERVED                      */
*         5 DCDSIZE,                /* DATA SET SIZE INFORMATION     */
*           6 DCDSPPRI FIXED(31),   /* PRIMARY SPACE AMOUNT          */
*           6 DCDSPSEC FIXED(31),   /* SECONDARY SPACE AMOUNT        */
*           6 DCDDIBLK FIXED(31),   /* DIRECTORY BLOCKS              */
*           6 DCDAVREC FIXED(8),    /* AVGREC -- M, K, U: SEE CONSTS */
*           6 *      CHAR(3),       /* RESERVED                      */
*           6 DCDAUNIT FIXED(31),   /* ALLOCATION UNIT AMOUNT        */
*           6 *        CHAR(4),     /* RESERVED                      */
*         5 DCDLRECL FIXED(31),     /* RECORD LENGTH                 */
*         5 DCDVSAM,                /* VSAM DATA SET PROPERTIES      */
*           6 DCDCISZ FIXED(31),    /* CISIZE FOR KS, ES OR RR       */
*           6 DCDFRSP,              /* FREESPACE                     */
*             7 DCDCIPCT FIXED(15), /* CI FREESPACE %                */
*             7 DCDCAPCT FIXED(15), /* CA FREESPACE %                */
*           6 DCDSHROP FIXED(15),   /* VSAM SHARE OPTIONS            */
*             7 DCDXSYS FIXED(8),   /* VSAM XSYSTEM SHARE OPTIONS    */
*             7 DCDXREG FIXED(8),   /* VSAM XREGION SHARE OPTIONS    */
*           6 DCDVINDX,             /* INDEX OPTIONS, ALL 0 = NONE   */
*             7 DCDIMBED BIT(1),    /* 1 = IMBED, 0 = NO             */
*             7 DCDREPLC BIT(1),    /* 1 = REPLICATE, 0 = NO         */
*             7 *      BIT(6),      /* RESERVED                      */
*           6 *       CHAR(1),      /* RESERVED                      */
*         5 DCDVOLCT FIXED(15),     /* MAXIMUM VOL COUNT FOR EXTEND  */
*         5 *      CHAR(2),         /* RESERVED                      */
*         5 DCDRETPD FIXED(31),     /* RETPD-TIME ACCESSIBLE TO SYS  */
*           6 DCDEXPYR FIXED(15),   /* EXPDT - YEAR                  */
*           6 DCDEXPDY FIXED(15),   /* EXPDT - ABSOLUTE DAY OF YEAR  */
*         5 * CHAR(20),             /* RESERVED                      */
*         5 * CHAR(0) BDY(DWORD),   /* ROUND TO DWORD BOUNDARY       */
*     /* CONSTANTS FOR DCDRCORG                                      */
*   DCDORGKS  CONSTANT(1),          /* RECORG IS VSAM KSDS           */
*   DCDORGES  CONSTANT(2),          /* RECORG IS VSAM ESDS           */
*   DCDORGRR  CONSTANT(3),          /* RECORG IS VSAM RRDS           */
*   DCDORGLS  CONSTANT(4),          /* RECORG IS VSAM LDS            */
*   DCDORGNL  CONSTANT(0),          /* RECORG IS NULL - SAM          */
*     /* CONSTANTS FOR DCDRECFM                                      */
*   DCDFMNUL  CONSTANT(0),          /* RECFM IS NULL                 */
*   DCDFMU    CONSTANT(1),          /* RECFM IS UNDEFINED            */
*   DCDFMV    CONSTANT(2),          /* RECFM IS VARIABLE             */
*   DCDFMVS   CONSTANT(3),          /* RECFM IS VARIABLE SPANNED     */
*   DCDFMVB   CONSTANT(4),          /* RECFM IS VARIABLE BLOCKED     */
*   DCDFMVBS  CONSTANT(5),          /* RECFM IS VARIABLE BLKD SPANNED*/
*   DCDFMF    CONSTANT(6),          /* RECFM IS FIXED                */
*   DCDFMFS   CONSTANT(7),          /* RECFM IS FIXED STANDARD       */
*   DCDFMFB   CONSTANT(8),          /* RECFM IS FIXED BLOCKED        */
*   DCDFMFBS  CONSTANT(9),          /* RECFM IS FIXED BLKED SPANNED  */
*     /* CONSTANTS FOR DCDCNTL                                       */
*   DCDCNTLA  CONSTANT(1),          /* CARRIAGE CONTROL IS ANSI      */
*   DCDCNTLM  CONSTANT(2),          /* CARRIAGE CONTROL IS MACHINE   */
*   DCDCNTLN  CONSTANT(3),          /* CARRIAGE CONTROL IS NULL      */
*     /* CONSTANTS FOR DCDAVREC                                      */
*   DCDBYTES FIXED(8) CONSTANT(1),  /* AVGREC IS BYTES               */
*   DCDKB    FIXED(8) CONSTANT(2),  /* AVGREC IS KB                  */
*   DCDMB    FIXED(8) CONSTANT(3),  /* AVGREC IS MB                  */
*   DCDTYP FIXED(15) CONSTANT(8),   /* CONSTANT LIST TYPE            */
*   DCDID  CHAR(8) CONSTANT('IGDDCD  '),/* CONSTANT MACRO ID         */
*   DCDVER FIXED(15) CONSTANT(0);   /* CONSTANT MACRO VERSION        */
* %GOTO MDCDF2;                     /*
