DCDFKOFF EQU   B'00001000'              KEYOFF SPECIFIED FLAG
DCDFEXP  EQU   B'00000100'              EXPIRATION ATTRIB SPEC'D FLAG
DCDFRET  EQU   B'00000010'              RETENTION ATTRIB SPEC'D FLAG
DCDFPSP  EQU   B'00000001'              PRIMARY SPACE SPECIFIED FLAG
DCDFSSP  EQU   B'10000000'              SECONDARY SPACE SPEC'D FLAG
DCDFDIR  EQU   B'01000000'              DIRECTORY BLOCKS SPEC'D FLAG
DCDFAUN  EQU   B'00100000'              ALLOCATION UNIT SPEC'D FLAG
DCDFAVR  EQU   B'00010000'              AVGREC SPECIFIED FLAG
DCDFVOL  EQU   B'00001000'              VOLUME CNT SPECIFIED FLAG
DCDFCIS  EQU   B'00000100'              CIZIZE SPECIFIED FLAG
DCDFCIF  EQU   B'00000010'              FREE CI % SPECIFIED FLAG
DCDFCAF  EQU   B'00000001'              FREE CA % SPECIFIED FLAG
DCDFXREG EQU   B'10000000'              SHAREOPT XREGION SPEC'D FLAG
DCDFXSYS EQU   B'01000000'              SHAREOPT XSYSTEM SPEC'D FLAG
DCDFIMBD EQU   B'00100000'              VSAM IMBED SPECIFIED FLAG
DCDFRPLC EQU   B'00010000'              VSAM REPLICATE SPECIFIED FLAG
         DS    CL1                      RESERVED
DCDKEY   DS    CL4                      VSAM KEY INFORMATION
DCDKOFF  DS    H                        VSAM KEY OFFSET
DCDKLEN  DS    AL1                      VSAM KEY LENGTH
         DS    AL1                      RESERVED
DCDSIZE  DS    CL24                     DATA SET SIZE INFORMATION
DCDSPPRI DS    F                        PRIMARY SPACE AMOUNT
DCDSPSEC DS    F                        SECONDARY SPACE AMOUNT
DCDDIBLK DS    F                        DIRECTORY BLOCKS
DCDAVREC DS    AL1                      AVGREC M, K, U  SEE CONSTS
         DS    CL3                      RESERVED
DCDAUNIT DS    F                        ALLOCATION UNIT AMOUNT
         DS    CL4                      RESERVED
DCDLRECL DS    F                        RECORD LENGTH
DCDVSAM  DS    CL12                     VSAM DATA SET PROPERTIES
DCDCISZ  DS    F                        CISIZE FOR KS, ES OR RR
DCDFRSP  DS    CL4                      FREESPACE
DCDCIPCT DS    H                        CI FREESPACE %
DCDCAPCT DS    H                        CA FREESPACE %
DCDSHROP DS    H                        VSAM SHARE OPTIONS
DCDXSYS  DS    AL1                      VSAM XSYSTEM SHARE OPTIONS
DCDXREG  DS    AL1                      VSAM XREGION SHARE OPTIONS
DCDVINDX DS    CL1                      INDEX OPTIONS, ALL 0 = NONE
DCDIMBED EQU   B'10000000'              1 = IMBED, 0 = NO
DCDREPLC EQU   B'01000000'              1 = REPLICATE, 0 = NO
         DS    CL1                      RESERVED
DCDVOLCT DS    H                        MAXIMUM VOL COUNT FOR EXTEND
         DS    CL2                      RESERVED
DCDRETPD DS    F                        RETPD TIME ACCESSIBLE TO SYS
DCDEXPYR DS    H                        EXPDT YEAR
DCDEXPDY DS    H                        EXPDT ABSOLUTE DAY OF YEAR
         DS    CL20                     RESERVED
         DS    CL32                     RESERVED
         DS    0C                       ROUND TO DWORD BOUNDARY
DCDE_LENGTH  EQU *-DCDNMLEN             LENGTH OF ENTRY            +RS
DCDORGKS EQU   1                        RECORG IS VSAM KSDS
DCDORGES EQU   2                        RECORG IS VSAM ESDS
DCDORGRR EQU   3                        RECORG IS VSAM RRDS
DCDORGLS EQU   4                        RECORG IS VSAM LDS
DCDORGNL EQU   0                        RECORG IS NULL SAM
***********************************************************************
*        CONSTANTS FOR DCDRECFM                                       *
***********************************************************************
DCDFMNUL EQU   0                        RECFM IS NULL
DCDFMU   EQU   1                        RECFM IS UNDEFINED
DCDFMV   EQU   2                        RECFM IS VARIABLE
DCDFMVS  EQU   3                        RECFM IS VARIABLE SPANNED
DCDFMVB  EQU   4                        RECFM IS VARIABLE BLOCKED
DCDFMVBS EQU   5                        RECFM IS VARIABLE BLKD SPANNED
DCDFMF   EQU   6                        RECFM IS FIXED
DCDFMFS  EQU   7                        RECFM IS FIXED STANDARD
DCDFMFB  EQU   8                        RECFM IS FIXED BLOCKED
DCDFMFBS EQU   9                        RECFM IS FIXED BLKED SPANNED
***********************************************************************
*        CONSTANTS FOR DCDCNTL                                        *
***********************************************************************
DCDCNTLA EQU   1                        CARRIAGE CONTROL IS ANSI
DCDCNTLM EQU   2                        CARRIAGE CONTROL IS MACHINE
DCDCNTLN EQU   3                        CARRIAGE CONTROL IS NULL
***********************************************************************
*        CONSTANTS FOR DCDAVREC                                       *
***********************************************************************
DCDBYTES EQU   1                        AVGREC IS BYTES
DCDKB    EQU   2                        AVGREC IS KB
DCDMB    EQU   3                        AVGREC IS MB
DCDTYP   EQU   8                        CONSTANT LIST TYPE
DCDVER   EQU   0                        CONSTANT MACRO VERSION
*
          MEND  ,  */  %MDCDF2: ;
./ ADD NAME=IGDLBD
         MACRO
         IGDLBD
IGDLBDH  DSECT                         Optical library header       +JM
LBDHID   DS    CL8                     ID = 'IGDLBD'
         DS    F                       Unknown
LBDPCNT  DS    F                       Count of LBD entries
         DS    F                       Unknown
LBDELNG  DS    F                       Length of LBD entry
LBDH_LENGTH EQU *-LBDHID               Length of header
IGDLBDE  DSECT                         IGDLBD
LBDNAML  DS    H                       LENGTH Optical lib
LBDNAME  DS    CL30                    Optical lib
LBDUSER  DS    CL8                     USERID
LBDDATE  DS    CL10                    YYYY/MM/DD
         DS    CL6                     Unknown
LBDTIME  DS    CL8                     HH:MM
         DS    CL2176                  ?
LBDE_LENGTH EQU *-LBDNAML              Length of entry
         MEND
./ ADD NAME=IGDMCD
* %GOTO MCDF1;                      /*
                                       MACRO
                                       IGDMCD
                                       AGO   .SSXXASM
*                                                                    */
* %MCDF1: ;
*/********************************************************************/
*/*  MACRO NAME: IGDMCD                                              */
*/*                                                                  */
*/*  DESCRIPTION: MANAGEMENT CLASS DEFINITION MAPPING                */
*/*                                                                  */
*/*  PERSON RESPONSIBLE: R. L. FERZIGER                              */
*/*                                                                  */
*/*  STATUS: DFP/XA 3.1.1                                    @YA27857*/
*/*                                                                  */
*/*  5665-XA3                                                        */
*/*  CONTAINS RESTRICTED MATERIALS OF IBM                            */
*/*  (C) COPYRIGHT IBM CORP. 1988                                    */
*/*  LICENSED MATERIALS - PROPERTY OF IBM                            */
*/*  REFER TO COPYRIGHT INSTRUCTIONS                                 */
*/*  FORM NUMBER G120-2083.                                          */
*/*                                                                  */
*/*  FUNCTION: MAPS THE MANAGEMENT CLASS DEFINITIONS.                */
*/*                                                                  */
*/*  NOTES:                                                          */
*/*                                                                  */
*/*    DEPENDENCIES: NONE                                            */
*/*    RESTRICTIONS: BECAUSE GETMAINS AND FREEMAINS MAY BE DONE      */
*/*      FOR THE INFORMATION MAPPED BY THIS MACRO, THE MACRO MUST    */
*/*      BE FORCED TO A DOUBLEWORD BOUNDARY.                         */
*/*                                                                  */
*/*  MACRO:                                                          */
*/*                                                                  */
*/*    PROCESSOR: PLAS/ASSEMBLER                                     */
*/*    MACRO SIZE: 90                                                */
*/*    DISTRIBUTION LIBRARY: MACLIB                                  */
*/*    ATTRIBUTES: BILINGUAL                                         */
*/*                                                                  */
*/*      THIS MACRO PRODUCES A PLS STRUCTURE OR AN ASSEMBLER DSECT   */
*/*      WHICH DESCRIBES THE FORMAT OF MANAGEMENT CLASSES.           */
*/*                                                                  */
*/*  EXIT NORMAL:                                                    */
*/*                                                                  */
*/*    RETURN CODE: 0                                                */
*/*    NO ERRORS DETECTED                                            */
*/*                                                                  */
*/*  EXIT ERRORS:                                                    */
*/*                                                                  */
*/*      NO ERRORS WILL BE DETECTED BY THIS MACRO.                   */
*/*                                                                  */
*/*  EXTERNAL REFERENCES:                                            */
*/*                                                                  */
*/*    MACROS:                                                       */
*/*      NONE                                                        */
*/*                                                                  */
*/*  CHANGE ACTIVITY:                                                */
*/* $P1=OY27857,JDP3311,900227,STLDGN: SWITCH MEANINGS (I.E.    @P1A */
*/*                                    COMMENTS) OF FIELDS      @P1A */
*/*                                    FOR RETAIN ONLY BACK UP  @P1A */
*/*                                    DAYS AND RETAIN EXTRA    @P1A */
*/*                                    BACKUPS.                 @P1A */
*/* $P2=OY31342,HDP3310,900405,STLDGN: ADD A 3RD CHOICE FOR     @P2A */
*/*                                    PARTIAL RELEASE OF SPACE.@P2A */
*/*                                    3RD CHOICE IS PARTIALLY  @P2A */
*/*                                    RELEASE SPACE ONLY IF    @P2A */
*/*                                    A SECONDARY ALLOC EXISTS.@P2A */
*/* 10/29/90 - LACK OF CONTINUATION CHARACTERS CAUSES ASSEMBLY       */
*/*            ERRORS.  ASSEMBLER SECTION ONLY.                      */
*/* $02=OY54802,JDP3311,920526,SJPLRLF: ADD LENGTH CONSTANTS     @02A*/
*/*     FOR CALLERS OF CNS (PL/AS ONLY!)                         @02A*/
*/********************************************************************/
*@EJECT M;
*/********************************************************************/
*/*                                                                  */
*/*      SMS MANAGEMENT CLASS CONSTRUCT DEFINITION                   */
*/*                                                                  */
*/********************************************************************/
* DECLARE
*   /* MANAGEMENT CLASS DEFINITION                                   */
*   1 MCD BASED BDY(DWORD),         /* MANAGEMENT CLASS MAPPING      */
*     2 MCDP,                       /* MANAGEMENT CLASS PREFIX       */
*       3 MCDPID CHAR(8),           /* ID = 'IGDMCD'                 */
*       3 *       CHAR(2),          /* UNUSED                        */
*       3 MCDPVER FIXED(15),        /* VERSION OF MACRO              */
*       3 MCDPCNT FIXED(31),        /* NUMBER OF MC DEFS - COUNT     */
*       3 MCDPTYP FIXED(15),        /* TYPE OF ITEMS IN LIST         */
*       3 *       CHAR(2),          /* RESERVED                      */
*       3 MCDPLEN FIXED(31),        /* LENGTH OF ONE DEF -- MCDEF    */
*     2 * (*) BDY(DWORD),
*      3 MCDEF BDY(DWORD),          /* ARRAY OF MGMT CLASS DEFS      */
*       4 MCDNM,                    /* SPACE FOR NAME AND LENGTH     */
*         5 MCDNMLEN FIXED(15),     /* LENGTH OF NAME                */
*         5 MCDNAME CHAR(30),       /* NAME OF MANAGEMENT CLASS      */
*       4 MCDUSER CHAR(8),          /* USERID OF LAST UPDATER        */
*       4 MCDDATE CHAR(10),         /* DATE OF LAST UPDATE           */
*       4 *       CHAR(6),          /* RESERVED                      */
*       4 MCDTIME CHAR(8),          /* TIME OF LAST UPDATE           */
*       4 MCDDESC CHAR(120),        /* DESCRIPTION                   */
*       4 MCDSUBDF,                 /* SUB DEFINITION                */
*         5 MCDFLAGS CHAR(3),       /* FLAGS AND RESERVED            */
*           6 MCDRELE BIT(2),       /* PARTIAL RELEASE CHOICES   @P2A*/
*             7 MCDPREL BIT(1),     /* DATA SET ELIGIBLE FOR PARTIAL */
*                                     /* RELEASE, 1=YES, 0=NO    @P2C*/
*             7 MCDPRCN BIT(1),     /*CONDITIONAL PARTIAL RELEASE@P2A*/
*           6 *     BIT(6),         /* RESERVED                  @P2C*/
*           6 MCDSPEC CHAR(2),      /* ATTRIBUTE SPECIFIED FLAGS:    */
*                                   /*  1= SPECIFIED, 0= NOT SPEC'D  */
*             7 MCDFBVER BIT(1),    /* WAS MCDBKVS SPECIFIED?        */
*             7 MCDFBVRD BIT(1),    /* WAS MCDBVRD SPECIFIED?        */
*             7 MCDFRBK BIT(1),     /* WAS MCDBKDY SPECIFIED?        */
*             7 MCDFRNP BIT(1),     /* WAS MCDBKNP SPECIFIED?        */
*             7 MCDFEXDT BIT(1),    /* WAS MCDEXDAT SPECIFIED?       */
*             7 MCDFEXDY BIT(1),    /* WAS MCDEXPDY SPECIFIED?       */
*             7 MCDFPRDY BIT(1),    /* WAS MCDPRDY SPECIFIED?        */
*             7 *        BIT(1),    /* RESERVED                      */
*             7 MCDFL1DY BIT(1),    /* WAS MCDL1DY SPECIFIED?        */
*             7 MCDFRLMG BIT(1),    /* WAS MCDRLOMG SPECIFIED?       */
*             7 MCDFPELE BIT(1),    /* WAS MCDPELEM SPECIFIED?       */
*             7 MCDFBKFQ BIT(1),    /* WAS MCDBKFQ SPECIFIED?        */
*             7 *        BIT(4),    /* RESERVED                      */
*         5 * CHAR(1),              /* RESERVED                      */
*   /*****************************************************************/
*   /* DATA SET MIGRATION ATTRIBUTES --                              */
*   /* NO MIGRATION IS ALLOWED IF MCDCMAU = MCDMNONE.                */
*   /* MCDPRDY IS VALID IF MCDFPRDY IS ON.                           */
*   /* MCDL1DY IS VALID IF MCDFL1DY IS ON AND MCDL1NOL IS OFF. VALUE */
*   /*     IS NOLIMIT IF MCDL1NOL IS ON.                             */
*   /*****************************************************************/
*         5 MCDMIGRT BDY(WORD),
*           6 MCDMIGF CHAR(1),      /* DATA SET MIGRATION FLAGS      */
*             7 *        BIT(1),    /* RESERVED                      */
*             7 *        BIT(1),    /* RESERVED                      */
*             7 MCDL1NOL BIT(1),    /* MIN DAYS ON LEVEL 1/LAST USE, */
*                                   /* 1=NOLIMIT,  ELSE SEE MCDL1DY  */
*             7 *      BIT(5),      /* RESERVED                      */
*           6 * CHAR(1),            /* RESERVED                      */
*         5 MCDPRDY FIXED(15),      /* MIN DAYS ON PRIM / LAST USE   */
*         5 MCDL1DY FIXED(15),      /* MIN DAYS ON LVL 1 / LAST USE  */
*         5 MCDCMAU FIXED(8),       /* COMMAND OR AUTO MIGRATE --    */
*                                   /* SEE CONSTANTS BELOW           */
*         5 *       CHAR(1),        /* RESERVED                      */
*   /*****************************************************************/
*   /* GENERATION DATA GROUP ATTRIBUTES --                           */
*   /* MCDRLOMG IS VALID IF MCDFRLMG IS ON.                          */
*   /* MCDPELEM IS VALID IF MCDFPELE IS ON.                          */
*   /*****************************************************************/
*         5 MCDGDG   BDY(WORD),
*           6 MCDGDGFL CHAR(1),     /* GDG ATTRIBUTE FLAGS           */
*             7 MCDRLOMG BIT(1),    /* MIGRATE OR EXPIRE ROLLED OFF  */
*                                   /* GDS, 1 = MIGRATE, 0 = EXPIRE  */
*             7 *        BIT(7),    /* RESERVED                      */
*           6 *        CHAR(1),     /* RESERVED                      */
*           6 MCDPELEM FIXED(15),   /* NUM GDG ELEMENTS ON PRIMARY   */
*   /*****************************************************************/
*   /*   ACTION ON DATA SET EXPIRATION (RESERVED FOR FUTURE USE)     */
*   /*****************************************************************/
*         5 MCDEXPIR BDY(WORD),
*           6 *      CHAR(4),       /* RESERVED                      */
*   /*****************************************************************/
*   /* DATA SET RETENTION ATTRIBUTES --                              */
*   /* MCDEXPDY IS VALID IF MCDFEXDY IS ON AND MCDDYNOL IS OFF.      */
*   /*     VALUE IS NOLIMIT IF MCDDYNOL IS ON.                       */
*   /* MCDEXDAT IS VALID IF MCDFEXDT IS ON AND MCDDTNOL IS OFF.      */
*   /*     VALUE IS NOLIMIT IF MCDDTNOL IS ON. THIS FIELD IS A       */
*   /*     NUMBER OF DAYS IF MCDRFMT = MCDFDAYS AND IS A DATE IF     */
*   /*     MCDRFMT = MCDFDATE.                                       */
*   /*****************************************************************/
*         5 MCDRETEN BDY(WORD),
*           6 MCDRETF CHAR(1),      /* DATA SET RETENTION FLAGS      */
*             7 MCDDYNOL BIT(1),    /* 1=EXPIRE AFTER DAYS= NOLIMIT  */
*                                     /* ELSE 0 AND SEE MCDEXPDY     */
*             7 MCDDTNOL BIT(1),    /* 1=EXPIRE AFTER DATE= NOLIMIT  */
*                                     /* ELSE 0 AND SEE MCDEXDAT     */
*             7 *     BIT(6),       /* RESERVED                      */
*           6 MCDRFMT FIXED(8),     /* FORMAT USED FOR MCDEXDAT --   */
*                                   /* DATE OR DAYS: SEE CONSTANTS   */
*           6 *       CHAR(2),      /* RESERVED                      */
*           6 MCDEXPDY FIXED(31),   /* EXPIRE AFTER DAYS NO USE      */
*           6 MCDEXDAT FIXED(31),   /* EXPIR. DAYS SINCE CREATE  OR  */
*             7 MCDEYEAR FIXED(16), /* EXPIRE DATE SINCE CREATE --   */
*             7 MCDEDAY FIXED(16),  /* SEE MCDRFMT TO FOR FORMAT     */
*   /*****************************************************************/
*   /* DATA SET BACKUP CRITERIA --                                   */
*   /* MCDBKFQ IS VALID IF MCDFBKFQ IS ON.                           */
*   /* MCDBKVS IS VALID IF MCDFBVER IS ON.                           */
*   /* MCDBVRD IS VALID IF MCDFBVRD IS ON.                       @P1C*/
*   /* MCDBKDY (NUMBER OF DAYS) IS VALID IF MCDFRNP (FLAG) IS    @P1C*/
*   /* ON AND MCDNPNOL (NO LIMIT) IS OFF.                        @P1C*/
*   /*     VALUE IS NOLIMIT IF MCDNPNOL IS ON.                   @P1C*/
*   /* MCDBKNP (NUMBER OF DAYS) IS VALID IF MCDFRBK (FLAG) IS    @P1C*/
*   /* ON AND MCDRBNOL (NO LIMIT) IF OFF.                        @P1C*/
*   /*     VALUE IS NOLIMIT IFF MCDRBNOL IS ON.                  @P1C*/
*   /* AUTO BACKUP IS NOT ALLOWED IF MCDAUTBK IS OFF.                */
*   /* NO COMMAND BACKUP IS ALLOWED IF MCDBADU = MCDBNONE. IF        */
*   /*     MCDBADU=MCDBBOTH BOTH ADMIN AND USER COMMAND ALLOWED. IF  */
*   /*     MCDBADU=MCDBADM ONLY ADMIN COMMAND BACKUP IS ALLOWED.     */
*   /*****************************************************************/
*         5 MCDBKUP,                /* DATA SET BACKUP CRITERIA      */
*           6 *     CHAR(1),        /* FLAGS AND RESERVED            */
*             7 MCDRBNOL BIT(1),    /*1=>RETAIN DAYS ONLY BACKUP VERS=
*                                        NOLIMIT. 0=>SEE MCDBKNP FOR
*                                        DAYS TO KEEP ONLY BACKUP@P1C*/
*             7 MCDNPNOL BIT(1),    /*1=>RETAIN DAYS EXTRA BACKUP VERS
*                                       = NOLIMIT. 0=>SEE MCDBKDY FOR
*                                       DAYS TO KEEP EXTRA BACKUP@P1C*/
*             7 *        BIT(1),    /* RESERVED                      */
*             7 MCDAUTBK BIT(1),    /* 1=AUTO BACKUP ALLOWED, ELSE 0 */
*             7 *      BIT(4),      /* RESERVED                      */
*           6 * CHAR(3),            /* RESERVED                      */
*           6 MCDBKFQ FIXED(15),    /* BACKUP FREQUENCY              */
*           6 MCDBKVS FIXED(15),    /* NUMBER OF BACKUP VERSIONS     */
*           6 MCDBVRD FIXED(15),    /* NUM OF VERSIONS: DS DELETED   */
*           6 MCDBKDY FIXED(15),    /* DAYS TO KEEP BACKUP VERSION   */
*           6 MCDBKNP FIXED(15),    /* DAYS TO KEEP ONLY BACKUP      */
*           6 MCDBADU FIXED(8),     /* ALLOW ADMIN OR USER BACKUP -- */
*                                   /* SEE CONSTANTS BELOW           */
*           6 *        CHAR(1),     /* RESERVED                      */
*           6 *        CHAR(8),     /* RESERVED FOR BACKUP DEST CLASS*/
*   /*****************************************************************/
*   /* MAXIMUM RETENTION CRITERIA --                                 */
*   /* LIMITS USER SPECIFIABLE EXPIRATION DATE AND RETENTION         */
*   /* PERIOD. NOLIMIT IF MCDRPNOL IS ON, ELSE VALUE IN MCDMRTDY.    */
*   /*****************************************************************/
*         5 MCDMRET,                /* MAXIMUM RETENTION CRITERIA    */
*           6 MCDMRETF CHAR(1),     /* MAXIMUM RETENTION FLAGS       */
*             7 MCDRPNOL BIT(1),    /* RETPD (RETAIN PD) 1=NOLIMIT   */
*                                     /* ELSE SEE MCDMRTDY       @P1C*/
*             7 *      BIT(7),      /* RESERVED                      */
*           6 * CHAR(1),            /* RESERVED                      */
*           6 MCDMRTDY FIXED(15),   /* MAXIMUM DAYS TO RETAIN        */
*         5 * CHAR(20),             /* RESERVED                      */
*         5 * CHAR(0) BDY(DWORD);   /* ROUND TO DWORD BOUNDARY       */
*   /*****************************************************************/
*   /*  CONSTANTS                                                    */
*   /*****************************************************************/
* DECLARE
*      /*  CONSTANTS FOR MCDRFMT                                     */
*   MCDNULL   CONSTANT(0),          /* FIELD WAS NOT USED            */
*   MCDFDATE  CONSTANT(1),          /* EXPIRE FORMAT: DATE/CREATE    */
*   MCDFDAYS  CONSTANT(2),          /* EXPIRE FORMAT: DAYS/CREATE    */
*      /*  CONSTANTS FOR MCDCMAU                                     */
*   MCDMNONE  CONSTANT(0),          /* NO MIGRATION ALLOWED          */
*   MCDMCMD   CONSTANT(1),          /* MIGRATE ON COMMAND ONLY       */
*   MCDMBOTH  CONSTANT(2),          /* AUTO MIGRATE OR ON COMMAND    */
*      /*  CONSTANTS FOR MCDBADU                                     */
*   MCDBNONE  CONSTANT(0),          /* NO USER OR ADMIN BACKUP       */
*   MCDBADM   CONSTANT(1),          /* ALLOW ADMIN COMMAND BACKUP    */
*   MCDBBOTH  CONSTANT(2),          /* ALLOW ADMIN OR USER COMMAND   */
*      /*  CONSTANTS FOR MCDRELE                                 @P2A*/
*   MCDRELYE  BIT(2) CONSTANT('10'B), /* PARTIAL RELEASE = YES.  @P2A*/
*   MCDRELCN  BIT(2) CONSTANT('01'B),/* PARTIAL REL=CONDITIONAL  @P2A*/
*   MCDRELNO  BIT(2) CONSTANT('00'B),/* PARTIAL RELEASE = NO.    @P2A*/
*      /*  OTHER CONSTANTS                                           */
*   MCDLEN  FIXED(31) CONSTANT(LENGTH(MCDP) + LENGTH(MCDEF)), /* @02A*/
*   MCDLENP  FIXED(31) CONSTANT(LENGTH(MCDP)),                /* @02A*/
*   MCDLENDF FIXED(31) CONSTANT(LENGTH(MCDEF)),               /* @02A*/
*   MCDTYP FIXED(15) CONSTANT(7),   /* CONSTANT LIST TYPE            */
*   MCDID CHAR(8) CONSTANT('IGDMCD  '),/* CONSTANT MACRO ID          */
*   MCDVER FIXED(15) CONSTANT(0);   /* CONSTANT MACRO VERSION        */
* %GOTO MMCDF2;                     /*
.SSXXASM  ANOP
IGDMCD    DSECT
***********************************************************************
