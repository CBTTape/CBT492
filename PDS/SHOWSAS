    //SYSIN    DD  *
      DATE(2000001,2008366)
      INDD(SMFIN,OPTIONS(DUMP))
      OUTDD(SMFOUT,TYPE(232))

STEP 3. (SAS) SAS CODE TO SHOW THE SHOWMVS SMF RECORDS
******************************************************
     OPTIONS ERRORABEND NOCENTER SOURCE SOURCE2;
    DATA TEST;
      FORMAT SMFSTAMP DATETIME19.2;
      INFILE SMF LENGTH=L;
      INPUT @002 RTY PIB1. @;
      SMFLEN=L;
      IF RTY EQ 232 THEN DO;
         I+1;
         IF I LE 5 THEN LIST;  *DUMP FIRST 5 SMF232 IN HEX TO LOG;
         INPUT
         @003 SMFSTAMP SMFSTAMP8.
         @011 SYSID    $CHAR4.
         @015 DATLEN   PIB1.
         @016 LINENUM  PIB4.
         @020 RECORD   $CHAR124.
         ;
         OUTPUT;
      END;
    RUN;
    PROC SORT DATA=TEST;
     BY SMFSTAMP LINE;
    PROC PRINT NOOBS;
     TITLE 'DATA LINES ONLY';
     VAR RECORD;
    PROC PRINT NOOBS;
     TITLE 'EVERYTHING ELSE';
     VAR SMFSTAMP SYSID SMFLEN DATLEN LINENUM;
